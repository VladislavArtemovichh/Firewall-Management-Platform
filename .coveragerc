[run]
source = app
omit = 
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*
    */migrations/*
    */alembic/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    def main\(\):
    if TYPE_CHECKING:
    from __future__ import annotations
    import __future__
    raise ImportError
    except ImportError:
    if not TYPE_CHECKING:
    if typing.TYPE_CHECKING:
    from typing import TYPE_CHECKING
    from typing_extensions import TYPE_CHECKING

[html]
directory = htmlcov 